{
  "name": "eliza",
  "scripts": {
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --apply .",
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm -r run build",
    "build:full": "pnpm --filter @elizaos/core run build && pnpm --filter \"@elizaos/*\" --filter \"!@elizaos/core\" run build",
    "build-docker": "turbo run build",
    "build:clients": "pnpm --filter \"!@elizaos/telegram-client\" -r run build",
    "build:telegram": "pnpm --filter @elizaos/telegram-client run build",
    "build:telegram-client": "pnpm --filter @elizaos/telegram-client run build",
    "clean": "rimraf packages/**/dist",
    "validate:workspace": "pnpm list --depth=0 --recursive",
    "cleanstart": "if [ -f packages/agent/data/db.sqlite ]; then rm packages/agent/data/db.sqlite; fi && pnpm --filter \"@elizaos/agent\" start --isRoot",
    "cleanstart:debug": "if [ -f packages/agent/data/db.sqlite ]; then rm packages/agent/data/db.sqlite; fi && cross-env NODE_ENV=development VERBOSE=true DEFAULT_LOG_LEVEL=debug DEBUG=eliza:* pnpm --filter \"@elizaos/agent\" start --isRoot",
    "start": "pnpm --filter @elizaos/agent start -- --character=characters/aengel.json",
    "start:client": "pnpm --dir client dev",
    "start:debug": "cross-env NODE_ENV=development VERBOSE=true DEFAULT_LOG_LEVEL=debug DEBUG=eliza:* pnpm --filter @elizaos/agent start -- --character=characters/aengel.json",
    "dev": "pnpm --filter @elizaos/agent start -- --character=characters/aengel.json",
    "release": "pnpm build && pnpm format && npx lerna publish --no-private --force-publish",
    "docker:build": "bash ./scripts/docker.sh build",
    "docker:run": "bash ./scripts/docker.sh run",
    "docker:bash": "bash ./scripts/docker.sh bash",
    "docker:start": "bash ./scripts/docker.sh start",
    "docker": "pnpm docker:build && pnpm docker:run && pnpm docker:bash",
    "test": "bash ./scripts/test.sh",
    "smokeTests": "bash ./scripts/smokeTests.sh",
    "integrationTests": "bash ./scripts/integrationTests.sh",
    "check:types": "tsc --noEmit",
    "check:cycles": "madge --circular --extensions ts packages/ --warning",
    "clean:all": "pnpm clean && find . -name \"*.tsbuildinfo\" -exec rm {} \\; 2>/dev/null || true",
    "validate:build": "pnpm -r run build && find packages/*/dist -name \"*.d.ts\" | grep -v \"node_modules\" | sort",
    "check:integrity": "pnpm dedupe && pnpm install --frozen-lockfile",
    "build:ci": "pnpm run clean:all && pnpm install && pnpm run build:telegram-client && pnpm --filter \"!@elizaos/telegram-client\" --filter \"!@elizaos/client-direct\" --filter \"!@elizaos/plugin-bootstrap\" --filter \"!@elizaos/agent\" -r run build",
    "verify": "pnpm run build:ci && pnpm run check:types",
    "build:active": "pnpm --filter='@elizaos/core' --filter='@elizaos/adapter-sqlite' --filter='@elizaos/agent' --filter='@elizaos/client-direct' --filter='@elizaos/client-telegram' --filter='@elizaos/clients/telegram' --filter='dynamic-imports' --filter='@elizaos/plugin-bootstrap' -r run build",
    "build:all": "pnpm -r run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.3",
    "@types/better-sqlite3": "^7.6.12",
    "@types/body-parser": "1.19.5",
    "@types/commander": "2.12.5",
    "@types/cors": "2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.14",
    "@types/json5": "2.2.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.14",
    "@types/node-telegram-bot-api": "^0.61.11",
    "@types/path-to-regexp": "^1.7.0",
    "@types/sqlite3": "^5.1.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@types/yargs": "17.0.33",
    "better-sqlite3": "11.9.1",
    "body-parser": "^2.2.0",
    "cli": "workspace:*",
    "concurrently": "9.1.0",
    "cors": "^2.8.5",
    "cross-env": "7.0.3",
    "esbuild": "^0.25.1",
    "express": "^5.1.0",
    "fs": "0.0.1-security",
    "fs-extra": "^11.3.0",
    "glob": "11.0.0",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "lerna": "8.1.5",
    "madge": "^8.0.0",
    "multer": "1.4.5-lts.2",
    "node-telegram-bot-api": "^0.61.0",
    "nodemon": "3.1.7",
    "only-allow": "1.2.1",
    "path": "^0.12.7",
    "rimraf": "^6.0.1",
    "sqlite-vec": "0.1.7-alpha.2",
    "ts-node": "^10.9.2",
    "tsup": "8.3.5",
    "tsx": "^4.7.0",
    "turbo": "2.4.4",
    "typedoc": "0.26.11",
    "typescript": "5.1.6",
    "unique-names-generator": "4.7.1",
    "uuid": "^11.1.0",
    "viem": "2.21.58",
    "vite": "5.4.12",
    "vitest": "3.0.5"
  },
  "pnpm": {
    "overrides": {
      "onnxruntime-node": "1.20.1",
      "@solana/web3.js@1.95.5": "npm:@solana/web3.js@1.95.5",
      "@solana/web3.js@1.95.8": "npm:@solana/web3.js@1.95.8",
      "@solana/web3.js@2": "npm:@solana/web3.js@2.0.0",
      "viem": "2.21.58",
      "@polkadot/util": "12.6.2",
      "@polkadot/util-crypto": "12.6.2",
      "@polkadot/types-create": "10.13.1",
      "@polkadot/types-codec": "10.13.1",
      "@polkadot/keyring": "12.6.2",
      "@ai-sdk/provider": "1.0.6",
      "@ai-sdk/provider-utils": "2.1.6",
      "cookie": "0.7.0",
      "bs58": "5.0.0",
      "@coral-xyz/anchor": "0.28.0",
      "axios@>=0.8.1 <0.28.0": ">=0.28.0",
      "undici@>=6.0.0 <6.21.1": ">=6.21.1",
      "path-to-regexp@<0.1.12": ">=0.1.12",
      "secp256k1": "5.0.1",
      "@octokit/request-error@>=1.0.0 <5.1.1": ">=5.1.1",
      "dompurify@<3.2.4": ">=3.2.4",
      "@octokit/request@>=1.0.0 <9.2.1": ">=9.2.1",
      "@octokit/plugin-paginate-rest@>=1.0.0 <11.4.1": ">=11.4.1",
      "esbuild@<=0.24.2": ">=0.25.0",
      "typescript": "5.1.6",
      "tsup": "8.3.5",
      "esbuild": "0.18.20",
      "@vercel/ai": "3.1.0"
    }
  },
  "engines": {
    "node": ">=18 <24",
    "pnpm": ">=8"
  },
  "dependencies": {
    "@0glabs/0g-ts-sdk": "0.2.1",
    "@ai-sdk/amazon-bedrock": "1.1.6",
    "@ai-sdk/anthropic": "1.1.6",
    "@ai-sdk/google": "1.1.0",
    "@ai-sdk/groq": "1.1.7",
    "@ai-sdk/mistral": "1.1.6",
    "@ai-sdk/openai": "1.1.9",
    "@coinbase/coinbase-sdk": "0.10.0",
    "@deepgram/sdk": "^3.9.0",
    "@elizaos-plugins/clients": "workspace:*",
    "@elizaos/adapter-sqlite": "workspace:*",
    "@elizaos/client-telegram": "0.25.6-alpha.1",
    "@injectivelabs/sdk-ts": "^1.14.33",
    "@vitest/eslint-plugin": "1.0.1",
    "ai": "4.1.16",
    "amqplib": "0.10.5",
    "bs58": "4.0.0",
    "commander": "^10.0.0",
    "csv-parse": "5.6.0",
    "dotenv": "^16.4.7",
    "json5": "^2.2.3",
    "langdetect": "^0.2.1",
    "ollama-ai-provider": "0.16.1",
    "optional": "0.1.4",
    "pnpm": "9.15.0",
    "sharp": "0.33.5",
    "sqlite": "5.0.1",
    "sqlite3": "5.1.6",
    "ws": "8.18.0",
    "yargs": "^17.0.33",
    "zod": "3.24.1",
    "@elizaos/plugin-bootstrap": "workspace:*",
